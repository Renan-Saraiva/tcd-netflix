{  
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Spring": {
    "Application": {
      "Name": "MoviesService"
    },
    "Cloud": {
      "Config": {
        "Uri": "http://configserver:8888",
        "ValidateCertificates": false,
        "fail_fast": true
      }
    },
    "RabbitMq": {
      "Host": "rabbitmq",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest"
    }
  },
  "Eureka": {
    "Client": {
      "ShouldRegisterWithEureka": true,
      "ServiceUrl": "http://eurekaserver:8761/eureka/",
      "ValidateCertificates": false
    },
    "Instance": {
      "AppName": "MoviesService",
      "HostName": "api-movies",
      "Port": "5100"
    }
  }
}