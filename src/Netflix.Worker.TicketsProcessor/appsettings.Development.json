{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Spring": {
      "Application": {
        "Name": "TicketsProcessor"
      },
      "Cloud": {
        "Config": {
          "Uri": "http://localhost:8888",
          "ValidateCertificates": false,
          "fail_fast": true,
          "Env": "Development"
        }
      }
    },
    "Eureka": {
      "Client": {
        "ShouldRegisterWithEureka": false,
        "ServiceUrl": "http://localhost:8761/eureka/",
        "ValidateCertificates": false
      }
    }
  }
}
